{
  "name": "dhi-atelier-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stage": "pm2 start ecosystem.config.js --env staging",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "prettier:all": "npx prettier --write .",
    "migrate": "npx sequelize-cli db:migrate",
    "seed-user": "npx sequelize-cli seed:generate --name admin-user",
    "seed-roles": "npx sequelize-cli seed:generate --name default-roles",
    "seed-modules": "npx sequelize-cli seed:generate --name default-modules",
    "seed-role-permissions": "npx sequelize-cli seed:generate --name default-role-permissions",
    "seed-material-colors": "npx sequelize-cli seed:generate --name default-material-colors",
    "seed-tokens": "npx sequelize-cli seed:generate --name default-tokens",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-fingerprint": "^1.2.2",
    "express-rate-limit": "^6.7.0",
    "express-useragent": "^1.0.15",
    "handlebars": "^4.7.7",
    "hbs": "~4.0.4",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.1.2",
    "nodemailer": "^6.9.1",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.1",
    "sequelize-cli": "^6.6.0",
    "sharp": "^0.31.1",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.1",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
